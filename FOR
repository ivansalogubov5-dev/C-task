#include<iostream>
using namespace std;

int main() {

	system("chcp 1251 > nul");

	int a;
	cout << "Enter number a: " << endl;
	cin >> a;
	cout << " " << endl;

	// From 1 to 10 (10 numbers)
	for (int i = 1; i <= a; i++) {
		cout << i << endl;
	}
	cout << " " << endl;
	// From 0 to 9 (10 numbers)
	for (int i = 0; i < a; i++) {
		cout << i << endl;
	}

	system("pause > nul");
	return 0;
}

